exam3 <- function(num1,d,num2){
result=0
if (d == "+"){
result = num1 + num2
}else if(d == "-"){
result = num1 - num2
}else if(d == "*"){
result = num1 * num2
}else if(d == "%/%"|"%%"){
if (num1 == 0) {
result = "오류1"
}else
result = num1%/%num2
}
}else(d == "%/%"|"%%")
if(num2==0){
result="오류2"
}else {
result=num1%%num2
}
}
return(result)
}
